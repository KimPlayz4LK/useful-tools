{
"manifest_version":2,
"name":"Draggables",
"description":"With this extension, you can add draggable notes to your page!",
"version":"1.0",
"permissions":["activeTab","tabs"],
"browser_action":{"default_icon":"icon.png","default_popup":"popup.html"},
"content_scripts":[{"matches":["<all_urls>"],"js":["background.js"]}]
}